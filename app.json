{
  "expo": {
    "name": "Derive",
    "slug": "derive",
    "version": "1.0.0",
    "orientation": "portrait",
    "backgroundColor": "#161622",
    "primaryColor": "#161622",
    "icon": "./assets/derive-icon.png",
    "splash": {
      "image": "./assets/splash-derive.png",
      "resizeMode": "cover",
      "backgroundColor": "#161622"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "backgroundColor": "#161622",
      "adaptiveIcon": {
        "foregroundImage": "./assets/derive-icon.png",
        "backgroundColor": "#161622"
      },
      "package": "Derive.apk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
